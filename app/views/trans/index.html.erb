<p id="notice"><%= notice %></p>

<h1>Trans</h1>

<table>
  <thead>
    <tr>
      <th>Source entity type</th>
      <th>Source entity</th>
      <th>Target entity type</th>
      <th>Target entity</th>
      <th>Type</th>
      <th>Amount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trans.each do |tran| %>
      <tr>
        <td><%= tran.source_entity_type %></td>
        <td><%= tran.source_entity_id %></td>
        <td><%= tran.target_entity_type %></td>
        <td><%= tran.target_entity_id %></td>
        <td><%= tran.trans_type %></td>
        <td><%= tran.amount %></td>
        <td><%= link_to 'Show', tran %></td>
        <td><%= link_to 'Edit', edit_tran_path(tran) %></td>
        <td><%= link_to 'Destroy', tran, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tran', new_tran_path %>
